{
    "configurations": [
        {
            "name": "Arduino",
            "includePath": [
                "${workspaceFolder}/**",
                "${env.HOME}/Arduino/libraries/**",
                "${env.HOME}/.arduino15/packages/arduino/tools/**",
                "${env.HOME}/.arduino15/packages/arduino/hardware/avr/1.8.6/variants/**",
                "${env.HOME}/.arduino15/packages/arduino/hardware/avr/1.8.6/cores/**",
                "${env.HOME}/.arduino15/packages/arduino/hardware/avr/1.8.6/libraries/**",
                "${env.HOME}/.arduino15/packages/arduino/hardware/avr/1.8.6/bootloaders/**",
                "${env.HOME}/.arduino15/packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/avr/include/**"
            ],
            "forcedInclude": [
                "${env.HOME}/.arduino15/packages/arduino/hardware/avr/1.8.6/cores/arduino/Arduino.h"
            ],
            "defines": [
                "CORE_RAM_SIZE=1024",
                "DEBUG=true",
                "USBCON"
            ],
            "compilerPath": "${env.HOME}/.arduino15/packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/bin/avr-g++",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-Wall"
            ],
            "cStandard": "gnu11",
            "cppStandard": "gnu++11"
        }
    ],
    "version": 4
}